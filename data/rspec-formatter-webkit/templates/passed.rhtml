<dd class="spec passed">
    <span class="spec-name"><%= h(example.description) %></span>
    <span class="spec-time"><%= h("%0.5fs" % [example.execution_result[:run_time]]) %></span>

% unless log_messages.nil? || log_messages.empty?
    <div class="log-messages">
        <%= log_messages.join("\n") %>
    </div>
% end
</dd>

